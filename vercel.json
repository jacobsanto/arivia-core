
{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "SUPABASE_URL": "@supabase_url",
    "SUPABASE_ANON_KEY": "@supabase_anon_key",
    "GUESTY_API_URL": "@guesty_api_url",
    "STRIPE_PUBLIC_KEY": "@stripe_public_key"
  },
  "build": {
    "env": {
      "SUPABASE_URL": "@supabase_url",
      "SUPABASE_ANON_KEY": "@supabase_anon_key",
      "GUESTY_API_URL": "@guesty_api_url",
      "STRIPE_PUBLIC_KEY": "@stripe_public_key"
    },
    "cmd": "npm run build",
    "dir": "."
  }
}
